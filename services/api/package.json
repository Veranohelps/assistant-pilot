    "eslint-plugin-react": "^7.23.2",
    "jest": "^26.6.3",
    "mock-req-res": "^1.2.0",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "sinon": "^10.0.0",
    "togeojson": "^0.16.0",
    "ts-jest": "^26.5.5",
